{
  "defaultSeverity": "error",
  "extends": [
      "tslint:recommended",
      "tslint-config-airbnb",
      "gts/tslint.json"
  ],
  "jsRules": {},
  "rules": {
    "eofline" : false, 
    "max-line-length" : [
      true,
      200
    ], 
    "align" : false,
    "no-boolean-literal-compare": false,
    "whitespace" : true,
  },
  "rulesDirectory": []
}