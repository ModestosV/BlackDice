{
  "defaultSeverity": "error",
  "extends": [
      "tslint:recommended",
      "tslint-config-airbnb",
      "gts/tslint.json"
  ],
  "rules": {
    "eofline" : false, 
    "max-line-length" : [
      true,
      200
    ], 
    "align" : false,
    "no-boolean-literal-compare": false,
    "whitespace" : true,
    "no-default-export" : false,
  }
}